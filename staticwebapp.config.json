{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{ico,json,xml,txt}", "/images/*.{png,jpg,,jpeg,gif}", "/static/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  }
}